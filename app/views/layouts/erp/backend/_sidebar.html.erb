<div class="page-sidebar-wrapper">
    <!-- BEGIN SIDEBAR -->
    <!-- DOC: Set data-auto-scroll="false" to disable the sidebar from auto scrolling/focusing -->
    <!-- DOC: Change data-auto-speed="200" to adjust the sub menu slide up/down speed -->
    <div class="page-sidebar navbar-collapse collapse">
        <!-- END SIDEBAR MENU -->
        <div class="page-sidebar-wrapper">
            <!-- BEGIN RESPONSIVE MENU FOR HORIZONTAL & SIDEBAR MENU -->
            <ul class="page-sidebar-menu page-header-fixed" data-keep-expanded="false" data-auto-scroll="true" data-slide-speed="200">
                <!-- DOC: To remove the search box from the sidebar you just need to completely remove the below "sidebar-search-wrapper" LI element -->
                <!-- BEGIN SIDEBAR TOGGLER BUTTON -->
                <li class="sidebar-toggler-wrapper hide">
                    <div class="sidebar-toggler">
                        <span></span>
                    </div>
                </li>
                <!-- END SIDEBAR TOGGLER BUTTON -->
                <!-- DOC: This is mobile version of the horizontal menu. The desktop version is defined(duplicated) in the header above -->
                <!-- DOC: To remove the search box from the sidebar you just need to completely remove the below "sidebar-search-wrapper" LI element -->
                <li class="nav-item start ">
                    <a href="<%= erp.backend_path %>" class="nav-link nav-toggle">
                        <i class="icon-home"></i>
                        <span class="title">Menu Quản Lý</span>
                    </a>
                </li>
                <%= render "layouts/erp/backend/menu/inventory" %>
                <%= render "layouts/erp/backend/menu/order" %>
                <%= render "layouts/erp/backend/menu/events" %>
                <%= render "layouts/erp/backend/menu/menu" %>
                <%= render "layouts/erp/backend/menu/article" %>
                <%= render "layouts/erp/backend/menu/comment_rating" %>
                <%= render "layouts/erp/backend/menu/contact" %>
                <%= render "layouts/erp/backend/menu/picture" %>
                <%= render "layouts/erp/backend/menu/area" %>                
                <%= render "layouts/erp/backend/menu/news_tes" %>
                <%= render "layouts/erp/backend/menu/system" %>
            </ul>
            <!-- END RESPONSIVE MENU FOR HORIZONTAL & SIDEBAR MENU -->
        </div>
    </div>
    <!-- END SIDEBAR -->
</div>